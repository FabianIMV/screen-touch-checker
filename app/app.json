{
  "expo": {
    "name": "screen-touch-checker",
    "slug": "screen-touch-checker",
    "owner": "fabianimv",
    "version": "1.0.0",
    "extra": {
      "eas": {
        "projectId": "bf691b35-395f-4381-a362-59a606507785"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/bf691b35-395f-4381-a362-59a606507785"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0F"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.touchdiagnostics.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Not used",
        "UIRequiresFullScreen": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0A0F"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.touchdiagnostics.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
